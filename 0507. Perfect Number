class Solution {
    public boolean checkPerfectNumber(int num) {
         
        if (num < 2) return false;
        int sum = 1;
        for (int x = 2; x * x <= num; x++)
        {
            if (num % x == 0)
            {
                sum += x;
                sum += num / x;
            }
        }
        
        return sum == num;
    
    }
}
